<!-- start form -->
<table border="0" width="100%" cellspacing="0" cellpadding="3">
  <tr>
    <td class="mainhead">
      $Env{"Box0"}$Text{"Responses <-> Attachments Management"}$Env{"Box1"}
    </td>
  </tr>
  <tr>
    <td align="center" class="mainbody">
      <br/>
      <table cellspacing="0" cellpadding="4" border="0" width="700">
        <tr class="contenthead">
          <td width="50%">
            $Text{"Response"}:$Text{"Attachment"} (1:n)
          </td>
          <td width="50%">
            $Text{"Response"}:$Text{"Attachment"} (n:1)
          </td>
        </tr>
        <tr class="contentbody">
          <td align="left">
            $Data{"AnswerQueueStrg"}
          </td>
          <td align="left">
            $Data{"QueueAnswerStrg"}
          </td>
        </tr>
        <tr class="contentfooter">
          <td class="contentvalue" colspan="2">&nbsp;</td>
        </tr>
      </table>
      <br/>
    </td>
  </tr>
</table>
<!-- end form -->
